RIGHT OUTER JOIN soubi_skill_tokusyukouka ON tokusyukouka = skill_tokusyukouka

WHERE tokusyukouka ISNULL
order by soubi</sql><sql name="SQL 19">DROP VIEW IF EXISTS hosei_tokusyukouka_variation;
